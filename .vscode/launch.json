    "version": "0.2.0",
    "configurations": [

        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "AplicacionConsultas",
            "request": "launch",
            "mainClass": "colectivo.controlador.AplicacionConsultas",
            "projectName": "colectivo-base"
        },
        {
            "type": "java",
            "name": "Run JavaFX",
            "request": "launch",
            "mainClass": "colectivo.gui.InterfazGrafica",
            "vmArgs": "--module-path ${workspaceFolder}/target/dependency --add-modules javafx.controls,javafx.fxml"
        }

    ]
}